{
    "name": "creads/api2symfony",
    "description": "PHP library which provide generation of Symfony mockup controllers based on spec",
    "type": "library",
    "keywords": ["php", "raml", "symfony", "api", "converter"],
    "license": "MIT",
    "homepage": "http://creads.fr",
    "authors": [
        {
            "name": "Quentin",
            "email": "q.pautrat@creads.org",
            "homepage": "https://github.com/qpautrat",
            "role": "Developer"
        }
    ],
    "require": {
        "alecsammon/php-raml-parser": "*@dev",
        "symfony/twig-bundle": "~2.6",
        "symfony/filesystem": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Creads\\Api2Symfony\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
